<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zx.dao.UserDao">
    <select id="findByName" parameterType="String" resultType="com.zx.domain.User">
        select * from tab_user where username = #{username};
    </select>

    <insert id="save" parameterType="user" >
        insert into tab_user values(null,#{username},
        #{password},#{name},#{birthday},#{sex},#{telephone},#{email},#{status},#{code})
    </insert>

</mapper>

